<script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>
<script>
  AOS.init();
</script>
<script>
  const btnKonfirmasiHadir = document.querySelector('#btnKonfirmasiHadir');
  const btnKirimUcapan = document.querySelector('#btnKirimUcapan');
  const btnSalinBSI = document.querySelector('#btnSalinBSI');
  const button = document.getElementById('myButton');
  const page = document.getElementById('page');
  const body = document.getElementsByTagName('body')[0];

  button.addEventListener('click', function () {
    page.classList.add('translate-y-full');
    body.classList.remove('overflow-y-hidden');
  });

  btnKonfirmasiHadir.addEventListener('click', (e) => {
    fetch(
      `${window.location.origin}/undangan/${e.srcElement.dataset.url}/presensi`,
      {
        method: 'POST',
        headers: {
          'Content-Type': 'application/json',
        },
        body: JSON.stringify({}),
      },
    )
      .then((response) => response.json())
      .then((data) => {
        alert(data.msg);
      })
      .catch((error) => {
        console.log(error);
      });
  });

  btnKirimUcapan.addEventListener('click', (e) => {
    e.preventDefault();
    fetch(
      `${window.location.origin}/undangan/${e.srcElement.dataset.url}/ucapan`,
      {
        method: 'POST',
        headers: {
          'Content-Type': 'application/json',
        },
        body: JSON.stringify({
          saying: document.querySelector('#inputUcapan').value,
        }),
      },
    )
      .then((response) => response.json())
      .then((data) => {
        alert(data.msg);
      })
      .catch((error) => {
        console.log(error);
      });
  });
  btnSalinBSI.addEventListener('click', (e) => {
    e.preventDefault();
    // Copy the text inside the text field
    navigator.clipboard.writeText(document.querySelector('#norek').innerHTML);
    // Alert the copied text
    // alert('Project role url copied ');
  });
</script>
